{
  "name": "ease",
  "version": "0.1.0",
  "description": "Sync local video between yourself and your loved ones for a more intimate experience",
  "main": "dist/src/main/App.js",
  "scripts": {
    "start": "electron .",
    "clean": "rimraf dist/*",
    "rebuild": "npm run clean && npm run build",
    "build": "npm run build:ts && npm run build:wp",
    "build:ts": "tsc --diagnostics --project .",
    "build:wp": "webpack",
    "watch": "concurrently --kill-others \"npm run watch:wp\" \"npm run watch:ts\" \"npm run watch:less\"",
    "watch:wp": "webpack -w",
    "watch:ts": "tsc -w",
    "watch:less": "less-watch-compiler src/render/style src/render/style",
    "test": "npm run test:main && npm run test:render",
    "test:main": "electron-mocha --recursive dist/test/main",
    "test:render": "electron-mocha --recursive --renderer dist/test/render --require-main dist/test/support/experimental-features.js",
    "test:debug-render": "electron-mocha -u tdd --debug --interactive --recursive --renderer dist/test/render --require-main dist/test/support/experimental-features.js",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "tslint ./src/**/*.ts*",
    "lint:src-fix": "tslint ./src/**/*.ts* --fix",
    "lint:test": "tslint ./test/**/*.ts*",
    "lint:test-fix": "tslint ./test/**/*.ts* --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Right2Drive/ease"
  },
  "pre-commit": [
    "lint",
    "rebuild"
  ],
  "keywords": [
    "electron",
    "video",
    "sync",
    "typescript",
    "videojs"
  ],
  "author": "Brad Pfannmuller",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^15.0.7",
    "@types/react-dom": "^0.14.23",
    "awesome-typescript-loader": "^3.0.3",
    "chai": "^3.5.0",
    "concurrently": "^3.3.0",
    "electron": "^1.4.15",
    "electron-mocha": "^3.3.0",
    "electron-react-devtools": "^0.4.0",
    "less": "^2.7.2",
    "less-watch-compiler": "^1.4.2",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "sinon-spy-react": "^2.0.1",
    "source-map-loader": "^0.1.6",
    "tslint": "^4.4.2",
    "tslint-react": "^2.3.0",
    "typescript": "^2.1.6",
    "typings": "^2.1.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "guid": "0.0.12",
    "material-design-lite": "^1.3.0",
    "purecss": "^0.6.2",
    "rc-slider": "^6.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-mdl": "^1.9.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "simple-peer": "^6.4.3",
    "socket.io-client": "^1.7.2",
    "spritesh": "^1.2.0",
    "svgxuse": "^1.2.2"
  }
}
